{
	"name": "saleor-storefront",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"license": "BSD-3-Clause",
	"scripts": {
		"dev": "node scripts/graceful-server.js vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node build/index.js",
		"predev": "svelte-kit sync && ./scripts/generate.sh",
		"prebuild": "svelte-kit sync && ./scripts/generate.sh",
		"generate": "export $(grep -v '^#' .env.development | xargs) 2>/dev/null || export $(grep -v '^#' .env | xargs) 2>/dev/null || true && graphql-codegen --config .graphqlrc.ts",
		"generate:clean": "npm config set verify-deps-before-run false && npm config set _jsr-registry '' && graphql-codegen --config .graphqlrc.ts",
		"sync": "svelte-kit sync",
		"setup": "svelte-kit sync && pnpm run generate",
		"postinstall": "./scripts/postinstall.sh",
		"prepare": "is-ci || (husky install && echo 'Husky hooks installed successfully')",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev:local": "export $(grep -v '^#' .env.development | xargs) && npm run dev",
		"start:dev": "export $(grep -v '^#' .env.development | xargs) && npm --silent run dev",
		"start:prod": "export $(grep -v '^#' .env.production | xargs) && npm --silent run start",
		"start:test": "export $(grep -v '^#' .env.test | xargs) && npm --silent run start",
		"serve:dev": "export $(grep -v '^#' .env.development | xargs) && vite dev --port 3000",
		"serve:prod": "export $(grep -v '^#' .env.production | xargs) && node build/index.js",
		"serve:test": "export $(grep -v '^#' .env.test | xargs) && node build/index.js",
		"install:browsers": "pnpm exec playwright install",
		"dev:docker": "./deploy.sh",
		"build:development": "export $(grep -v '^#' .env.development | xargs) && npm run build",
		"build:production": "cp svelte.config.local.js svelte.config.js && ./scripts/build-clean.sh .env.production",
		"build:test": "export $(grep -v '^#' .env.test | xargs) && npm run build",
		"build:local": "cp svelte.config.local.js svelte.config.js && npm run build",
		"test": "export $(grep -v '^#' .env.test | xargs) && npx playwright test",
		"test:local": "export $(grep -v '^#' .env.test | xargs) && BASE_URL=http://127.0.0.1:3000 npx playwright test",
		"test:docker": "./run-tests-docker.sh",
		"test:docker:full": "./test-docker.sh",
		"test:headed": "export $(grep -v '^#' .env.test | xargs) && npx playwright test --headed",
		"test:debug": "export $(grep -v '^#' .env.test | xargs) && npx playwright test --debug",
		"docker:deploy": "./deploy.sh",
		"docker:stop": "./stop.sh",
		"docker:logs": "./logs.sh",
		"docker:status": "./status.sh",
		"docker:dev": "./deploy.sh",
		"docker:prod": "NODE_ENV=production ./deploy.sh",
		"vercel:build": "pnpm run build",
		"vercel:dev": "vercel dev"
	},
	"dependencies": {
		"@adyen/adyen-web": "6.21.0",
		"@adyen/api-library": "29.0.0",
		"@esbuild/linux-x64": "^0.25.9",
		"@rollup/rollup-linux-x64-gnu": "^4.50.0",
		"@saleor/auth-sdk": "1.0.2",
		"@stripe/stripe-js": "7.9.0",
		"@sveltejs/kit": "^2.37.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"clsx": "2.1.1",

		"editorjs-html": "4.0.5",
		"libphonenumber-js": "1.12.15",
		"lodash-es": "4.17.21",
		"lucide-svelte": "^0.542.0",
		"query-string": "9.2.2",
		"svelte": "^5.38.6",
		"tailwindcss": "^3.4.17",
		"ts-invariant": "0.10.3",
		"url-join": "5.0.0",
		"urql": "5.0.1",
		"xss": "1.0.15",
		"yup": "1.7.0"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "5.0.7",
		"@graphql-codegen/client-preset": "4.8.3",
		"@graphql-typed-document-node/core": "3.2.0",
		"@parcel/watcher": "2.5.1",
		"@playwright/test": "1.55.0",
		"@sveltejs/adapter-node": "5.3.1",
		"@sveltejs/adapter-vercel": "5.10.2",
		"@sveltejs/vite-plugin-svelte": "^6.1.4",
		"@types/lodash-es": "4.17.12",
		"@types/node": "24.3.0",
		"@types/url-join": "5.0.0",
		"autoprefixer": "10.4.21",

		"graphql-tag": "2.12.6",
		"husky": "9.1.7",
		"is-ci": "4.1.0",
		"lint-staged": "16.1.6",
		"postcss": "8.5.6",
		"prettier": "3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "0.6.14",
		"schema-dts": "1.1.5",
		"svelte": "^5.0.0",
		"svelte-check": "^4.3.1",
		"typescript": "5.9.2",
		"vite": "^7.1.4",
		"wonka": "6.3.5"
	},
	"resolutions": {
		"graphql": "16.8.1"
	},
	"packageManager": "pnpm@10.14.0",
	"engines": {
		"pnpm": ">=9.4.0",
		"node": ">=20"
	},
	"scripts-info": {
		"package-managers": "This project supports pnpm (recommended), npm, and yarn. Use the package manager of your choice."
	}
}
